{
  "provider_cost_caps": {
    "trial_single_tenant_usd_per_month_cap": 100,
    "includes": [
      "vercel",
      "neon",
      "email_smtp",
      "object_storage"
    ],
    "soft_alert_at_percent": [
      70,
      85,
      95
    ],
    "hard_stop_at_percent": 100,
    "grace_minutes_default": 0
  },
  "ai_tiers": [
    {
      "name": "Eco",
      "max_tokens": 4096,
      "target_cost_per_1k_tokens_usd": 0.0006,
      "quality": "brief-bullets",
      "default": true
    },
    {
      "name": "Standard",
      "max_tokens": 16384,
      "target_cost_per_1k_tokens_usd": 0.0025,
      "quality": "balanced"
    },
    {
      "name": "Max",
      "max_tokens": 65536,
      "target_cost_per_1k_tokens_usd": 0.008,
      "quality": "rich, citations, reasoning"
    }
  ],
  "rate_limits": {
    "email_drafts_per_minute_per_tenant": "auto",
    "sms_per_minute_per_tenant": "auto",
    "ai_requests_per_minute_per_tenant": "auto",
    "burst_multiplier": 3
  },
  "auto_scaling_policy": {
    "grades": [
      {
        "tier": "S",
        "multiplier": 5
      },
      {
        "tier": "A",
        "multiplier": 3
      },
      {
        "tier": "B",
        "multiplier": 2
      },
      {
        "tier": "C",
        "multiplier": 1
      }
    ],
    "inputs": [
      "stripe_mrr",
      "daily_active_users",
      "error_rate",
      "p95_latency_ms",
      "ai_overruns",
      "payment_success_rate"
    ],
    "downgrade_cooldown_days": 14
  },
  "rbac_roles": {
    "provider": [
      "provider_admin",
      "provider_engineer",
      "provider_finance"
    ],
    "tenant": [
      "tenant_owner",
      "tenant_manager",
      "tenant_employee",
      "tenant_accountant",
      "tenant_it_partner"
    ]
  },
  "federation": {
    "subdomains": {
      "provider": "provider",
      "tenant_pattern": "{tenant}.",
      "customer_portal": "portal"
    },
    "jwt_audiences": {
      "provider": "provider",
      "tenant": "tenant"
    }
  },
